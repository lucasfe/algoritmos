   "source": [
    "numbers = [1,2,3,4,5,6,7]\n",
    "\n",
    "start = 0\n",
    "end = len(numbers) - 1\n",
    "while (start < end):\n",
    "    temp = numbers[start]\n",
    "    numbers[start] = numbers[end]\n",
    "    numbers[end] = temp\n",
    "    start += 1\n",
    "    end += -1\n",
    "print(numbers)"
   ]
